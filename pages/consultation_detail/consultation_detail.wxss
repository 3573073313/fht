/* pages/consultation_detail/consultation_detail.wxss */
.consultation_detailPage {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 0rpx 37rpx;
}

.consultation_detailPageTitle {
  width: 100%;
  height: auto;
  background-color: #fff;
  box-sizing: border-box;
  padding: 20rpx 0rpx;
}

.consultation_detailPageTitleCont {
  width: 100%;
  height: auto;
  min-height: 30rpx;
  color: #333;
  font-weight: bold;
  text-align: justify;
}

.consultation_detailPageTitleDate {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #bebebe;
  text-align: right;
}

.consultation_detailPageCont {
  width: 100%;
  height: auto;
  text-align: justify;
}